Vagrant.configure("2") do |config|
    config.vm.provider "docker" do |d|
      d.image = "rockyos-vagrant:9.1"
      # d.build_dir = "."
      d.has_ssh = true
      d.remains_running = true
      d.create_args = ["--privileged"]
    end
    config.vm.network "forwarded_port", id: "ssh", host: 2200, guest: 22
  end